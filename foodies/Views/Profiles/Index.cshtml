@using (Html.BeginForm("Index", "Profiles", FormMethod.Get))
{
    <div>
         @Html.TextBox("search",null, new {@placeholder = "Find Foodists..."})
        <input type="submit" value="Search" />
    </div>
}
<br />
<br />

@foreach (foodies.Models.ApplicationUser user in ViewBag.Users)
{
    if (user.Id != ViewBag.Profile.UserId)
    {
        <div class="panel panel-default">
            <a href="/Profiles/Show/@user.Id"><strong> @user.Nickname </strong></a>
        </div>
    }

}
